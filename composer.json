{
  "name": "clthck/mini-asset-improved",
  "description": "An asset compression library. Provides file concatenation and a flexible filter system for preprocessing and minification. Improved version of markstory/mini-asset.",
  "type": "cakephp-plugin",
  "keywords": ["cakephp", "assets", "minifier", "less", "coffee-script", "sass", "postcss"],
  "homepage": "https://github.com/clthck/mini-asset-improved",
  "license": "MIT",
  "authors": [
    {
      "name": "Mark Story",
      "homepage": "http://mark-story.com",
      "role": "Author"
    },
    {
      "name": "clthck",
      "homepage": "http://github.com/clthck",
      "role": "Contributor"
    }
  ],
  "support": {
    "issues": "https://github.com/clthck/mini-asset-improved/issues",
    "source": "https://github.com/clthck/mini-asset-improved"
  },
  "require": {
    "league/climate": "~3.0"
  },
  "require-dev": {
    "squizlabs/PHP_CodeSniffer": "*",
    "phpunit/phpunit": "4.1.*"
  },
  "autoload": {
    "psr-4": {
      "MiniAsset\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiniAsset\\Test\\": "tests"
    }
  },
  "bin": ["bin/mini_asset"],
  "suggest": {
    "natxet/CssMin": "For using the CssMin filter.",
    "tedivm/jshrink": "For using the JShrink filter.",
    "linkorb/jsmin-php": "For using the JSMin filter.",
    "leafo/scssphp": "For using the ScssPHP filter.",
    "oyejorge/less.php": "For using the LessDotPHP filter, see https://github.com/oyejorge/less.php"
  }
}
